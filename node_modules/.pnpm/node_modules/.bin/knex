#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Nacho/Desktop/Backend/node_modules/.pnpm/knex@2.5.1_mysql2@3.6.2/node_modules/knex/bin/node_modules:/mnt/c/Users/Nacho/Desktop/Backend/node_modules/.pnpm/knex@2.5.1_mysql2@3.6.2/node_modules/knex/node_modules:/mnt/c/Users/Nacho/Desktop/Backend/node_modules/.pnpm/knex@2.5.1_mysql2@3.6.2/node_modules:/mnt/c/Users/Nacho/Desktop/Backend/node_modules/.pnpm/node_modules:/mnt/c/Users/Nacho/Desktop/Backend/node_modules:/mnt/c/Users/Nacho/Desktop/node_modules:/mnt/c/Users/Nacho/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Nacho/Desktop/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Nacho/Desktop/Backend/node_modules/.pnpm/knex@2.5.1_mysql2@3.6.2/node_modules/knex/bin/node_modules:/mnt/c/Users/Nacho/Desktop/Backend/node_modules/.pnpm/knex@2.5.1_mysql2@3.6.2/node_modules/knex/node_modules:/mnt/c/Users/Nacho/Desktop/Backend/node_modules/.pnpm/knex@2.5.1_mysql2@3.6.2/node_modules:/mnt/c/Users/Nacho/Desktop/Backend/node_modules/.pnpm/node_modules:/mnt/c/Users/Nacho/Desktop/Backend/node_modules:/mnt/c/Users/Nacho/Desktop/node_modules:/mnt/c/Users/Nacho/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Nacho/Desktop/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../knex/bin/cli.js" "$@"
else
  exec node  "$basedir/../knex/bin/cli.js" "$@"
fi
